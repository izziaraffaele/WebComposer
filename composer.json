{
    "name": "izziaraffaele/webcomposer",
    "description": "A suite of PHP components that you can use to compose your application",
    "authors": [
        {
            "name": "Izzia Raffaele",
            "email": "izziaraffaele@gmail.com"
        }
    ],
    "minimum-stability":"dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3",
        "silex/silex": "~1.2",
        "symfony/debug": "~2.6",
        "silex/web-profiler": "~1.0",
        "symfony/monolog-bridge": "~2.6",
        "symfony/twig-bridge": "~2.6",
        "marcojanssen/silex-service-register-provider": "1.1.*",
        "marcojanssen/silex-routing-service-provider": "~1.2",
        "filp/whoops": "~1.1",
        "igorw/config-service-provider": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-0": {
            "WebComposer\\": "./src"
        }
    }
}